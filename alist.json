{
  "force": false,
  "address": "0.0.0.0",
  "port": 5244,
  "site_url": "${DOMAIN}",
  "cdn": "https://fastly.jsdelivr.net/npm/alist-web@$version/dist/",
  "jwt_secret": "",
  "token_expires_in": 720,
  "database": {
    "type": "postgres",
    "host": "alist_db",
    "port": 5432,
    "user": "alist",
    "password": "${POSTGRES_PASSWORD}",
    "name": "alist",
    "db_file": "data/data.db",
    "table_prefix": "x_",
    "ssl_mode": "disable"
  },
  "scheme": {
    "https": false,
    "cert_file": "",
    "key_file": ""
  },
  "temp_dir": "data/temp",
  "log": {
    "enable": true,
    "name": "log/log.log",
    "max_size": 10,
    "max_backups": 5,
    "max_age": 28,
    "compress": false
  }
}
